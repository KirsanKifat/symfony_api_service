{
    "name": "kirsankifat/symfony_api_service",
    "description": "Symfony api default service structure",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Svyatoslav Karamushko",
            "email": "karamushkoss@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": ">=2",
        "doctrine/orm": "^2",
        "jms/serializer-bundle": ">=3",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KirsanKifat\\ApiServiceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KirsanKifat\\ApiServiceBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6",
        "doctrine/doctrine-fixtures-bundle": "^3",
        "phpunit/phpunit": "^9",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/maker-bundle": "^1",
        "symfony/phpunit-bridge": "^6"
    }
}
